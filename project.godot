[gui]

theme/custom="res://Assets/Classic95.tres"
theme/custom_font="res://Assets/PixelOperator.ttf"

[rendering]

textures/canvas_textures/default_texture_filter=0
textures/vram_compression/import_etc2_astc=true
environment/defaults/default_clear_color=Color(0.764706, 0.780392, 0.796078, 1)
